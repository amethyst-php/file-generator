{
    "name": "railken/amethyst-file-generator",
    "keywords": ["laravel", "amethyst", "amethyst-file-generator", "file-generator"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-data-builder": "0.1.*",
        "railken/amethyst-template": "0.1.*",
        "railken/amethyst-file": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\FileGeneratorServiceProvider"
            ]
        }
    }
}
